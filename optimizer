    "print(\"\\n\" + \"=\"*80)\n",
    "print(\"ðŸŽ¯ SPECIFIC RECOMMENDATIONS FOR YOUR GOALS\")\n",
    "print(\"=\"*80)\n",
    "\n",
    "def find_sweet_spots(results, scenario_name):\n",
    "    print(f\"\\n{scenario_name} SCENARIO RECOMMENDATIONS:\")\n",
    "    conservative = results[(results['Initial_Sites'] >= 20) & (results['Initial_Sites'] <= 25)].nlargest(3, 'Value_Multiple') if 'Initial_Sites' in results.columns else pd.DataFrame()\n",
    "    if not conservative.empty:\n",
    "        best_conservative = conservative.iloc[0]\n",
    "        print(f\"\\nðŸ’¼ CONSERVATIVE APPROACH:\")\n",
    "        print(f\"   â€¢ Initial Sites: {best_conservative['Initial_Sites']}\")\n",
    "        print(f\"   â€¢ Initial Equity: {format_currency(best_conservative['Initial_Equity'])}\")\n",
    "        print(f\"   â€¢ Distribution Rate: {format_percentage(best_conservative['Distribution_Rate'])}\")\n",
    "        print(f\"   â€¢ Schedule: {best_conservative['Schedule']}\")\n",
    "        print(f\"   â€¢ Expected Combined Value: {format_currency(best_conservative['Combined_Value'])}\")\n",
    "        print(f\"   â€¢ Value Multiple: {format_multiple(best_conservative['Value_Multiple'])}\")\n",
    "        print(f\"   â€¢ IRR: {format_percentage(best_conservative['IRR'])}\")\n",
    "        print(f\"   â€¢ Break-even: Year {best_conservative['Break_Even_Year'] or 'N/A'}\")\n",
    "    moderate = results[(results['Initial_Sites'] >= 26) & (results['Initial_Sites'] <= 30)].nlargest(3, 'Combined_Value') if 'Initial_Sites' in results.columns else pd.DataFrame()\n",
    "    if not moderate.empty:\n",
    "        best_moderate = moderate.iloc[0]\n",
    "        print(f\"\\nâš–ï¸ MODERATE APPROACH:\")\n",
    "        print(f\"   â€¢ Initial Sites: {best_moderate['Initial_Sites']}\")\n",
    "        print(f\"   â€¢ Initial Equity: {format_currency(best_moderate['Initial_Equity'])}\")\n",
    "        print(f\"   â€¢ Distribution Rate: {format_percentage(best_moderate['Distribution_Rate'])}\")\n",
    "        print(f\"   â€¢ Schedule: {best_moderate['Schedule']}\")\n",
    "        print(f\"   â€¢ Expected Combined Value: {format_currency(best_moderate['Combined_Value'])}\")\n",
    "        print(f\"   â€¢ Value Multiple: {format_multiple(best_moderate['Value_Multiple'])}\")\n",
    "        print(f\"   â€¢ IRR: {format_percentage(best_moderate['IRR'])}\")\n",
    "        print(f\"   â€¢ Break-even: Year {best_moderate['Break_Even_Year'] or 'N/A'}\")\n",
    "    aggressive = results[(results['Initial_Sites'] >= 31) & (results['Initial_Sites'] <= 100)].nlargest(3, 'Combined_Value') if 'Initial_Sites' in results.columns else results.nlargest(3, 'Combined_Value')\n",
    "    if not aggressive.empty:\n",
    "        best_aggressive = aggressive.iloc[0]\n",
    "        print(f\"\\nðŸš€ AGGRESSIVE APPROACH:\")\n",
    "        print(f\"   â€¢ Initial Sites: {best_aggressive['Initial_Sites']}\")\n",
    "        print(f\"   â€¢ Initial Equity: {format_currency(best_aggressive['Initial_Equity'])}\")\n",
    "        print(f\"   â€¢ Distribution Rate: {format_percentage(best_aggressive['Distribution_Rate'])}\")\n",
    "        print(f\"   â€¢ Schedule: {best_aggressive['Schedule']}\")\n",
    "        print(f\"   â€¢ Expected Combined Value: {format_currency(best_aggressive['Combined_Value'])}\")\n",
    "        print(f\"   â€¢ Value Multiple: {format_multiple(best_aggressive['Value_Multiple'])}\")\n",
    "        print(f\"   â€¢ IRR: {format_percentage(best_aggressive['IRR'])}\")\n",
    "        print(f\"   â€¢ Break-even: Year {best_aggressive['Break_Even_Year'] or 'N/A'}\")\n",
    "\n",
    "if not low_results.empty:\n",
    "    find_sweet_spots(low_results, \"LOW REVENUE\")\n",
    "if not high_results.empty:\n",
    "    find_sweet_spots(high_results, \"HIGH REVENUE\")\n",
    "if not blended_results.empty:\n",
    "    find_sweet_spots(blended_results, \"BLENDED REVENUE\")\n",
    "if not variable_draw_results.empty:\n",
    "    find_sweet_spots(variable_draw_results, \"LOW REVENUE (VARIABLE EQUITY DRAW)\")"
   ]
